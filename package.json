{
  "name": "cloudflare-hono-waitlist-template",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev",
    "build": "bunx --bun astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier . --write",
    "format:check": "prettier --check .",
    "lint": "eslint 'src/**/*.{js,ts,tsx,astro}'",
    "worker": "wrangler dev",
    "dryrun:staging": "bun run build && wrangler deploy --dry-run --env staging",
    "dryrun:production": "bun run build && wrangler deploy --dry-run --env production",
    "deploy:staging": "bun run build && wrangler deploy --env staging",
    "deploy:production": "bun run build && wrangler deploy --env production"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.19",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.53.1",
    "astro": "^5.16.11",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "hono": "^4.11.4",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "prettier": "^3.8.0",
    "prettier-plugin-astro": "^0.14.1",
    "react": "^19.2.3",
    "react-compiler-runtime": "^1.0.0",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "wrangler": "^4.59.3"
  },
  "devDependencies": {
    "daisyui": "^5.5.14",
    "prettier-plugin-tailwindcss": "^0.7.2"
  }
}
